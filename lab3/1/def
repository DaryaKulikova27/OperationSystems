// Finite Automata
digraph {
	H [label=H]
	C [label=C]
	S [label=S]
	F [label=F]
	Z [label=Z]
	A [label=A]
	B [label=B]
	H -> C [label=1]
	H -> S [label=0]
	H -> F [label=0]
	H -> F [label=1]
	C -> C [label=1]
	C -> Z [label=0]
	C -> F [label=1]
	C -> F [label=0]
	S -> S [label=0]
	S -> F [label=0]
	Z -> S [label=0]
	Z -> A [label=1]
	Z -> F [label=0]
	A -> B [label=0]
	A -> C [label=1]
	A -> F [label=1]
	B -> A [label=1]
}
